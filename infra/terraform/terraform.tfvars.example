# Copy this file to terraform.tfvars and fill in real values
# terraform.tfvars is git-ignored to keep secrets and environment-specific settings out of VCS

region        = "us-east-1"
instance_type = "t3.micro"
# Must be an existing AWS EC2 key pair name in the chosen region
key_name      = "devopsapp-key"
project       = "devopsapp"
# For initial testing you can allow the world; tighten to your IP/CIDR later
allowed_cidrs = [
  "0.0.0.0/0"
]
